{
   "importConfig" : {
        "bulkSize" : 25,
        "indexField" : {"index":"%value%"},
        "indexPrefix" : "insights_default_",
        "action" : "index",
        "sourceField" : {"udm":"%value%"},
        "threadCount" : 32
   },
   "threadCount" : 4,
   "defaultIndexPattern":"insights_default_*",
   "structureIndexPattern":"insightsanalytics_structure*",
   "structureIndexPrefix":"insightsanalytics_structure_",
   "linkIndexPattern":"insightsanalytics_link*",
   "linkIndexPrefix":"insightsanalytics_link_",
   "classificationIndex":"insightssystem_classification",
   "indexMapping" : {
        "field" : {"udm":{"hierarchy":"%value"}},
        "type" : "matches"
   },
   "defaultIndexTemplate":{
      "index_patterns":[
         "insights_*",
         "insightssystem_classification"
      ],
      "settings":{
         "index":{
            "number_of_replicas":0,
            "refresh_interval":"30s"
         },
         "analysis":{
            "analyzer":{
               "singlePath":{
                  "tokenizer":"keyword"
               },
               "path":{
                  "type":"standard"
               },
               "length":{
                  "tokenizer":"length"
               },
               "depth":{
                  "tokenizer":"depth"
               },
               "tree":{
                  "tokenizer":"path_hierarchy"
               },
               "treeReverse":{
                  "tokenizer":"treeReverse"
               }
            },
            "tokenizer":{
               "length":{
                  "type":"nGram",
                  "min_gram":1,
                  "max_gram":1
               },
               "depth":{
                  "type":"simple_pattern",
                  "pattern":"/[^/]+"
               },
               "treeReverse":{
                  "type":"path_hierarchy",
                  "reverse":true
               }
            },
            "normalizer":{
               "lowercase":{
                  "type":"custom",
                  "filter":[
                     "lowercase"
                  ]
               }
            }
         }
      },
      "mappings":{
         "dynamic_templates":[
            {
               "date":{
                  "match_mapping_type":"date",
                  "match":"date*",
                  "mapping":{
                     "type":"date"
                  }
               }
            },
            {
               "principal":{
                  "match":"principal*",
                  "mapping":{
                     "type":"text",
                     "fields":{
                        "keyword":{
                           "type":"keyword"
                        }
                     }
                  }
               }
            },
            {
               "name":{
                  "match":"name*",
                  "mapping":{
                     "type":"text",
                     "fields":{
                        "keyword":{
                           "type":"keyword"
                        }
                     }
                  }
               }
            },
            {
               "analyticsClassification":{
                  "path_match":"analyticsClassification.*",
                  "mapping":{
                     "type":"keyword"
                  }
               }
            }
         ],
         "properties":{
            "id":{
               "type":"keyword"
            },
            "reversedVersionOrder":{
               "type":"integer"
            },
            "versionCount":{
               "type":"integer"
            },
            "type":{
               "type":"keyword"
            },
            "subType":{
               "type":"keyword"
            },
            "isFile" : {
                "type" : "boolean"
            },
            "source":{
               "type":"keyword"
            },
            "name":{
               "type":"text",
               "fields":{
                  "keyword":{
                     "type":"keyword",
                     "store" : true
                  },
                  "length":{
                     "type":"token_count",
                     "analyzer":"length",
                     "store":true
                  }
               }
            },
            "description":{
               "type":"text",
               "fields":{
                  "keyword":{
                     "type":"keyword"
                  }
               }
            },
            "state":{
               "type":"keyword"
            },
            "hierarchy":{
               "type":"text",
               "fields":{
                  "keyword":{
                     "type":"keyword",
                     "store":true
                  },
                  "length":{
                     "type":"token_count",
                     "analyzer":"length",
                     "store":true
                  },
                  "depth":{
                     "type":"token_count",
                     "analyzer":"depth",
                     "store":true
                  },
                  "tree":{
                     "type":"text",
                     "analyzer":"tree",
                     "search_analyzer":"singlePath"
                  },
                  "treeReverse":{
                     "type":"text",
                     "analyzer":"treeReverse",
                     "search_analyzer":"singlePath"
                  }
               }
            },
            "url" : {
                "type" : "keyword"
            },
            "parent":{
               "type":"keyword",
               "fields":{
                  "tree":{
                     "type":"text",
                     "analyzer":"tree",
                     "search_analyzer":"singlePath",
                     "fielddata":true
                  }
               }
            },
            "parentId" : {
                "type" : "keyword"
            },  
            "dateCreated":{
               "type":"date"
            },
            "dateModified":{
               "type":"date"
            },
            "dateAccessed":{
               "type":"date"
            },
            "principalCreated":{
               "type":"text",
               "fields":{
                  "keyword":{
                     "type":"keyword"
                  }
               }
            },
            "principalModified":{
               "type":"text",
               "fields":{
                  "keyword":{
                     "type":"keyword"
                  }
               }
            },
            "principalAccessed":{
               "type":"text",
               "fields":{
                  "keyword":{
                     "type":"keyword"
                  }
               }
            },
            "principalAuthor":{
               "type":"text",
               "fields":{
                  "keyword":{
                     "type":"keyword"
                  }
               }
            },
            "principalOwner":{
               "type":"text",
               "fields":{
                  "keyword":{
                     "type":"keyword"
                  }
               }
            },
            "binaryExtension":{
               "type":"keyword",
               "fields":{
                  "normal":{
                     "type":"keyword",
                     "normalizer":"lowercase",
                     "store":true
                  },
                  "length":{
                     "type":"token_count",
                     "analyzer":"length",
                     "store":true
                  }
               }
            },
            "binaryByteSize":{
               "type":"long"
            },
            "binaryByteSizeAsNumber":{
               "type":"long"
            },
            "binaryHash":{
               "type":"keyword"
            },
            "binaryPath":{
               "type":"keyword"
            },
            "metadata":{
               "type" : "object"
            },
            "analyticsBinaryHashCount":{
               "type":"integer"
            },
            "analyticsBinaryUnique":{
               "type":"boolean"
            },
            "analyticsBinaryParentUnique":{
               "type":"boolean"
            },
            "analyticsBinarySource":{
               "type":"keyword"
            },
            "analyticsHasChildren":{
               "type":"boolean"
            },
            "analyticsTranslationCount":{
               "type":"integer"
            },
            "analyticsAvailableTranslations":{
               "type":"keyword"
            },
            "analyticsClassificationQuery":{
               "type":"percolator"
            },
            "analyticsClassificationType":{
               "type":"keyword"
            },
            "analyticsClassificationResult":{
               "type":"keyword"
            },
            "cleanAction" : { 
                "type": "keyword"
            },
            "cleanResult" : { 
                "type": "keyword"
            },
            "cleanMessage" : { 
                "type": "keyword"
            },
            "deletedFromSource" : {
                "type" : "boolean"
            },
            "language" : {
                "type" : "keyword"
            },
            "isOriginalLanguage" : {
                "type" : "boolean"
            }
         }
      }
   },
   "structureIndexTemplate":{
      "index_patterns":[
         "insightsanalytics_structure*"
      ],
      "settings":{
         "index":{
            "number_of_replicas":0,
            "refresh_interval":"30s"
         },
         "analysis":{
            "analyzer":{
               "depth":{
                  "tokenizer":"depth"
               }
            },
            "tokenizer":{
               "depth":{
                  "type":"simple_pattern",
                  "pattern":"/[^/]+"
               }
            }
         }
      },
      "mappings":{
         "dynamic_templates":[
            {
               "date":{
                  "match":"date*",
                  "mapping":{
                     "type":"date"
                  }
               }
            },
            {
               "count":{
                  "match":"*Count",
                  "mapping":{
                     "type":"long"
                  }
               }
            },
            {
               "analyticsClassification":{
                  "path_match":"analyticsClassification.*",
                  "mapping":{
                     "type":"keyword"
                  }
               }
            }
         ],
         "properties":{
            "type":{
               "type":"keyword"
            },
            "source":{
               "type":"keyword"
            },
            "extension":{
               "type":"keyword"
            },
            "name":{
               "type":"keyword"
            },
            "size":{
               "type":"long"
            },
            "hierarchy":{
               "type":"text",
               "fields":{
                  "keyword":{
                     "type":"keyword",
                     "store":true
                  },
                  "depth":{
                     "type":"token_count",
                     "analyzer":"depth",
                     "store":true
                  }
               }
            }
         }
      }
   },
   "linkIndexTemplate" : {
       "index_patterns":[
         "insightsanalytics_link*"
      ],
      "settings":{
         "index":{
            "number_of_replicas":0,
            "refresh_interval":"30s"
         }
      }
   },
   "kibanaVersion" : "7.5.2"
}